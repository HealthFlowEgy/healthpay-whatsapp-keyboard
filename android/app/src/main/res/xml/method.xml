<?xml version="1.0" encoding="utf-8"?>
<!-- Input Method Configuration - v1.2.1 Fixed duplicate keyboard entries -->
<input-method
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:settingsActivity="tech.healthpay.keyboard.ui.SettingsActivity"
    android:icon="@drawable/ic_keyboard"
    android:isDefault="false">

    <subtype
        android:label="@string/keyboard_label_en"
        android:imeSubtypeLocale="en_US"
        android:imeSubtypeMode="keyboard"
        android:isAsciiCapable="true"
        android:icon="@drawable/ic_keyboard" />

    <subtype
        android:label="@string/keyboard_label_ar"
        android:imeSubtypeLocale="ar"
        android:imeSubtypeMode="keyboard"
        android:isAsciiCapable="false"
        android:icon="@drawable/ic_keyboard" />

</input-method>
